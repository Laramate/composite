{
    "name": "laramate/composite",
    "description": "Code Composite Tools for Laravel",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Laramate",
            "email": "coding@laramate.de",
            "homepage": "https://www.laramate.de/",
            "role": "Team"
        },
        {
            "name": "Tobi Kivelip",
            "email": "tkivelip@gmail.com",
            "homepage": "https://github.com/tkivelip",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "7.*|8.*",
        "laramate/facade-mapper": "*",
        "laramate/support": "*"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Laramate\\Composite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laramate\\Composite\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laramate\\Composite\\Providers\\CompositeProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../*"
        }
    ]
}
